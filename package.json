{
  "name": "powerpiper-contract-demo",
  "version": "0.0.1",
  "scripts": {
    "build": "webpack",
    "dev": "webpack-dev-server --host 0.0.0.0 --port 8080",
    "lint": "eslint ./",
    "lint:contract": "solium -d contracts/",
    "compile:local": "truffle compile --network=development --optimize-runs 200",
    "compile:ropsten": "truffle compile --network=ropsten --optimize-runs 200",
    "migrate:local": "truffle migrate --network=development --reset",
    "migrate:ropsten": "truffle migrate --network=ropsten --reset",
    "coverage": "./node_modules/.bin/solidity-coverage",
    "rpc": "ganache-cli --defaultBalanceEther=1000000000"
  },
  "author": "Tadas Talaikis <info@talaikis.com>",
  "license": "GPLv3",
  "dependencies": {
    "cross-env": "^5.1.4",
    "crypto": "^1.0.1",
    "dotenv": "^5.0.1",
    "kms-env": "^0.3.0",
    "truffle": "^4.1.3",
    "truffle-contract": "^1.1.11",
    "truffle-hdwallet-provider-privkey": "^0.1.0",
    "truffle-wallet-provider": "0.0.5",
    "zeppelin-solidity": "^1.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-bignumber": "^2.0.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "ganache-cli": "^6.1.0",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "mocha": "^5.0.4",
    "solidity-coverage": "^0.4.14",
    "solium": "^1.1.5",
    "style-loader": "^0.13.1",
    "web3": "^0.20.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
